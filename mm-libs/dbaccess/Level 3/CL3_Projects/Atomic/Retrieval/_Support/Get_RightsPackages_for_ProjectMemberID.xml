  usr_accounts.IsDeleted = 0 And
  tms_pro_projectmembers.IsDeleted = 0 And
  tms_pro_members_2_rightpackage.IsDeleted = 0 And
  tms_pro_acc_rightspackages.IsDeleted = 0 And
  tms_pro_projectmembers.Tenant_RefID = @TenantID
  
